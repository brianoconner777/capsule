      <meta name="twitter:description" content="<%- data[0].share_des %>">
      <meta name="twitter:creator" content="HumanFOX">
      <!-- Twitter summary card with large image must be at least 280x150px -->
      <meta name="twitter:image:src" content="<%- data[0].share_image %>">
      <!-- Open Graph data -->
      <meta property="og:title" content="<%- data[0].share_title %>" />
