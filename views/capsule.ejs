                                      <!-- Facebook,Twitter,Website,Contact -->
                                          <% var total_coa = data[0].capsule_coa.value.length;
                                              for(var i=0;i<total_coa;i++)
                                              {
                                              var type = data[0].capsule_coa.value[i].type;
                                              var value = data[0].capsule_coa.value[i].value;
                                              if(type=="facebook" || type=="Facebook")
                                              {
                                              %>
                                          <i class="fa fa-facebook color-teal"></i>
                                          <span class="main-font font-weight-300 color-teal">
                                                <a href="<%- value %>" target="_blank"><%- value %></a>
                                          </span><br/>
                                          <%
                                              }else if(type=="twitter" || type=="Twitter")
                                              {
                                              %>
                                              <i class="fa fa-twitter color-teal"></i>
                                          <span class="main-font font-weight-300 color-teal">
                                                <a href="<%- value %>" target="_blank"><%- value %></a>
                                          </span><br/>
                                              <%
                                              }else if(type=="contact" || type=="Contact")
                                              {
                                              %>
                                          <i class="fa fa-phone color-teal"></i>
                                          <span class="main-font font-weight-300 color-teal">
                                                <a href="tel:<%- value %>" target="_blank"><%- value %></a>
                                          </span><br/>
                                              <%
                                              }else if(type=="email" || type=="Email")
                                              {
                                              %>
                                              <i class="fa fa-envelope color-teal"></i>
                                          <span class="main-font font-weight-300 color-teal">
                                                <a href="mailto:<%- value %>" target="_blank"><%- value %></a>
                                          </span><br/>
                                          <%
                                              }
                                              else
                                              {
                                              %>
                                           <i class="fa fa-bolt color-teal"></i>
                                          <span class="main-font font-weight-300 color-teal">
                                                <a href="<%- value %>" target="_blank"><%- value %></a>
                                          </span><br/>
                                          <%
                                              }
                                              }

