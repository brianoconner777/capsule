      <title><%- data[0].capsule_name %>, <%- data[0].capsule_title%> | HumanFox</title>
      <meta name="viewport" content="width=device-width, initial-scale=1" />
      <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
      <!-- Robots -->
      <meta name="keywords" content="<%for(var i=0;i<data[0].capsule_tags.length;i++){%><%=data[0].capsule_tags[i].value%>,<%}%>"/>
      <meta name="description" content="<%- data[0].share_des %>" />
      <meta name="robots" content="index, follow" />
      <meta name="revisit-after" content="3 days" />
      <meta name="author" content="" />
      <meta name="robots" Content="all" />
      <meta name="googlebot" content="noodp" />
      <meta name="googlebot" content="noarchive" />
      <meta name="copyright" content="HumanFOX" />
