      <meta property="og:site_name" content="HumanFOX" />
      <meta property="article:published_time" content="<%- data[0].published_time; %>" />
      <meta property="article:modified_time" content="<%- data[0].modified_time; %>" />
      <meta property="article:section" content="Article Section" />
      <meta property="article:tag" content="" />
      <meta property="fb:app_id" content="292242034489396" />
      <meta property="fb:admins" content="" />
